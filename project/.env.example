# Mapbox API Token
# Get your token from https://account.mapbox.com/access-tokens/
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_token_here

# FullCAM API Subscription Key (optional)
# Only needed if using the FullCAM API
VITE_FULLCAM_SUBSCRIPTION_KEY=your_subscription_key_here

# API Proxy URL (for production)
# Point to your deployed proxy server
VITE_API_PROXY_URL=http://localhost:3001
